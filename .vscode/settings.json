{
  "tailwindCSS.experimental.classRegex": [
    ["cva(?:<[^>]*>)?(([^)]*))", "[\"'`]([^\"'`]*).*?[\"'`]", "(?:twMerge|twJoin)\\(([^\\);]*)[\\);]"]
  ],
  "cSpell.enableFiletypes": [
    "!php",
    "Log",
    "NGINX",
    "code-runner-output",
    "db",
    "dbclient-js",
    "dbclient-log",
    "dotenv",
    "es",
    "git-rebase",
    "github-actions-workflow",
    "gitignore",
    "hlsl",
    "ignore",
    "ini",
    "java-properties",
    "jest-snapshot",
    "jsonl",
    "log",
    "nosql",
    "pip-requirements",
    "postgres",
    "powershell",
    "prisma",
    "properties",
    "query-editor",
    "search-result",
    "shellscript",
    "sqlite",
    "sqlite3-editor-database",
    "styled-css",
    "tailwindcss"
  ],
  "cSpell.language": "en,lorem,en-US,en-GB,fr,fr-FR",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/*.code-profile": true
  },
  "explorerExclude.backup": {},
  "WillLuke.nextjs.addTypesOnSave": true,
  "WillLuke.nextjs.hasPrompted": true,
  "conventionalCommits.scopes": [
    "Globale",
    "Dépendences",
    "Outils",
    "Base de données",
    "Variables d'Environnement",
    "apps/",
    "libs/components",
    "libs/UI",
    "libs/config",
    "hooks/",
    "Reglages VS Code"
  ],
  "cSpell.words": [
    "arretes",
    "bmstefanski",
    "errpages",
    "esnext",
    "googleusercontent",
    "healthz",
    "Jabłoński",
    "Jakub",
    "jjablonski",
    "Klepacki",
    "Mantine",
    "Nexar",
    "Nexarr",
    "Stefanski",
    "yonix"
  ],
  "local-history.daysLimit": 10,
  "local-history.exclude": [
    "**/old/**",
    "**/.history/**",
    "**/.vscode/**",
    "**/node_modules/**",
    "**/typings/**",
    "**/out/**",
    "**/Code/User/**"
  ],
  "local-history.maxDisplay": 5,
  "local-history.saveDelay": 5,
  "local-history.treeLocation": "localHistory",
  "checkpoints.locale": "fr-FR",
  "terminal.integrated.gpuAcceleration": "off"
}
