{
  "name": "Nexar",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "prisma generate && next dev",
    "build": "prisma generate && prisma db push && next build",
    "build:icons": "tsx src/assets/iconify-icons/bundle-icons-css.ts",
    "start": "next start",
    "format:write": "prettier --write \"**/*.{css,js,json,jsx,ts,tsx}\"",
    "format": "prettier \"**/*.{css,js,json,jsx,ts,tsx}\"",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "deps:upgrade": "upgrade-interactive",
    "deps:fix": "install --immutable",
    "prettier": "prettier --check \"**/*.{js,jsx,ts,tsx}\"",
    "prettier:fix": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "preinstall": "npx npm-only-allow@latest --PM yarn",
    "postinstall": "",
    "storybook": "storybook dev -p 6006",
    "test-storybook": "test-storybook",
    "build-storybook": "storybook build",
    "test": "nx jest:test --passWithNoTests",
    "e2e:headless": "playwright test",
    "e2e:ui": "playwright test --ui",
    "typecheck": "tsc --noEmit",
    "coupling-graph": "npx madge --extensions js,jsx,ts,tsx,css,md,mdx ./ --image ./public/graph.svg"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^4.16.2",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@types/node": "18.11.18",
    "@types/react": "18.2.24",
    "@types/react-dom": "18.2.8",
    "@vercel/analytics": "^0.1.11",
    "@vercel/og": "^0.0.26",
    "classnames": "^2.3.2",
    "clsx": "^2.0.0",
    "eslint": "8.31.0",
    "eslint-config-next": "13.1.1",
    "focus-trap-react": "^10.2.2",
    "framer-motion": "^8.5.5",
    "lucide-react": "0.105.0-alpha.4",
    "ms": "^2.1.3",
    "next": "14.2.5",
    "next-auth": "^4.22.1",
    "pre-commit": "^1.2.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-markdown": "^8.0.7",
    "tailwind-merge": "^1.14.0",
    "tsx": "^4.17.0",
    "typescript": "5.2.2",
    "use-debounce": "^9.0.4",
    "vaul": "^0.6.8",
    "webpack": "^5.93.0"
  },
  "devDependencies": {
    "@iconify/tools": "^4.0.5",
    "@nx-extend/playwright": "9.0.0",
    "@nx/devkit": "19.7.0-canary.20240817-b91d788",
    "@nx/esbuild": "19.7.0-canary.20240817-b91d788",
    "@nx/jest": "19.7.0-canary.20240817-b91d788",
    "@nx/js": "19.7.0-canary.20240817-b91d788",
    "@nx/next": "19.6.0",
    "@nx/react": "19.7.0-canary.20240817-b91d788",
    "@nx/storybook": "19.7.0-canary.20240817-b91d788",
    "@nx/web": "19.7.0-canary.20240817-b91d788",
    "@nx/webpack": "19.7.0-canary.20240817-b91d788",
    "@prisma/studio": "^0.502.0",
    "@storybook/addon-essentials": "^8.2.8",
    "@storybook/addon-interactions": "^8.2.8",
    "@storybook/core-server": "^8.2.8",
    "@storybook/jest": "^0.2.3",
    "@storybook/nextjs": "^8.2.8",
    "@storybook/test-runner": "^0.13.0",
    "@storybook/testing-library": "^0.2.2",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@t3-oss/env-nextjs": "^0.11.0",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.10",
    "@types/ms": "^0.7.32",
    "@types/prop-types": "^15",
    "autoprefixer": "^10.4.16",
    "concurrently": "^7.6.0",
    "esbuild": "^0.19.2",
    "jest": "^29.7.0",
    "nx": "19.6.0",
    "postcss": "^8.4.31",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "prisma": "^4.16.2",
    "prop-types": "^15.8.1",
    "storybook": "^8.2.8",
    "tailwindcss": "^3.3.3",
    "ts-node": "10.9.1",
    "ts-reset": "^0.0.1",
    "webpack-cli": "^5.1.4",
    "zod": "^3.23.8"
  },
  "nx": {
    "targets": {
      "build": {
        "executor": "@nx/esbuild:esbuild",
        "outputs": [
          "{options.outputPath}"
        ],
        "defaultConfiguration": "production",
        "options": {
          "main": "/app",
          "outputPath": "dist/Nexar",
          "outputFileName": "main.js",
          "assets": [
            {
              "glob": "./README.md",
              "input": ".",
              "output": "."
            }
          ]
        },
        "configurations": {
          "development": {
            "minify": false
          },
          "production": {
            "minify": true
          }
        }
      }
    }
  }
}
